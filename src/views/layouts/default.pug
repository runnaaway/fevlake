            a(href='linux.html') linux
            a(href='devops.html') devops
            a(href='google.html') google
            a(href='yandex.html') yandex
            a(href='clickhouse.html') clickhouse
            a(href='mysql.html') mysql
            a(href='kubernetes.html') kubernetes
            a(href='docker.html') docker
